<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hcmute.nhom03.foodyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodyApp">
        <activity
            android:name=".CartActivity"
            android:exported="false"/>
        <activity
            android:name=".OrderActivity"
            android:exported="false"/>
        <activity
            android:name=".FoodInfoActivity"
            android:exported="false" />
        <activity
            android:name=".RestaurantInfoActivity"
            android:exported="false"
            android:parentActivityName=".RestaurantsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".RestaurantsActivity"/>
        </activity>
        <activity
            android:name=".RestaurantsActivity"
            android:exported="false" />
        <activity
            android:name=".WelcomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".AccountSettingActivity"/>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".InfoAccountActivity"/>
        <activity android:name=".EditAccountActivity"/>
        <activity android:name=".ForgotPassActivity"/>

    </application>

</manifest>